{
  "name": "war3icon",
  "displayName": "war3icon",
  "description": "在线war3图标库，可直接导入到项目中，自动生成量暗图标，支持方案边框设置，并支持自定义导入",
  "icon": "images/gitlab-logo-128.png",
  "publisher": "shiyue1023261581",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "war3icon.items",
        "title": "War3 Icons",
        "icon": "$(file-media)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "war3icon.items",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "war3icon",
      "properties": {
        "war3icon.rscPath": {
          "type": "string",
          "default": "resource",
          "description": "项目资源目录"
        },
        "war3icon.FramekeyValuePairs": {
          "type": "object",
          "description": "方案配置，第一个是亮，第二个是暗，第三个是被动，每个要用#号隔开",
          "additionalProperties": {
            "type": "string",
            "description": "输入值"
          },
          "default": {
            "方案1：默认": "${插件路径}/images/bm_btn.png#${插件路径}/images/bm_dis.png#${插件路径}/images/bm_pas.png"
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "war3-model": "^2.2.1"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}